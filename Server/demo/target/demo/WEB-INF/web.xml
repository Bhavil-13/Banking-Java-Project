<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>





  <servlet>
    <servlet-name>controlServlet</servlet-name>
    <servlet-class>example</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>controlServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>



  <servlet>
    <servlet-name>CreateAPerson</servlet-name>
    <servlet-class>CreateAPerson</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CreateAPerson</servlet-name>
    <url-pattern>/createAPerson</url-pattern>
  </servlet-mapping>




<servlet>
    <servlet-name>CreateAccount</servlet-name>
    <servlet-class>CreateAccount</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CreateAccount</servlet-name>
    <url-pattern>/createAccount</url-pattern>
  </servlet-mapping>




</web-app>